#!raku

%*ENV<SP6_DUMP_TASK_CODE> = False;

bash "git checkout --", %(
   cwd => "{%*ENV<HOME>}/.sparky",
);

git-scm tags()<SCM_URL>, %(
  to => "{%*ENV<HOME>}/.sparky",
  branch => tags<SCM_BRANCH>
);

#bash "sparman.raku worker_ui stop; sparman.raku worker_ui start; sparman.raku worker_ui status";
